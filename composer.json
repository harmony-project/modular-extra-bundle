{
  "name": "harmony-project/modular-extra-bundle",
  "type": "library",
  "description": "Adds additional functionality to the Symfony framework for projects that support modular routing",
  "keywords": ["routing", "database", "symfony"],
  "homepage": "http://harmony-project.io",
  "license": "MIT",
  "authors": [
    {
      "name": "Tim Goudriaan",
      "homepage": "http://codedmonkey.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "harmony-project/modular-bundle": "^1.0@dev",
    "symfony/symfony": "^3.0",
    "symfony-cmf/routing-bundle": "^2.0",

    "harmony-project/modular-routing": "^1.0@dev"
  },
  "require-dev": {
    "doctrine/orm": "^2.4",
    "phpunit/phpunit": ">=6.0",
    "sensio/framework-extra-bundle": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Harmony\\Bundle\\ModularExtraBundle\\": "source/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
